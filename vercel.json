{
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "outputDirectory": "build",
    "functions": {
      "api/*.js": {
        "maxDuration": 25
      }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}
